# FastAgent Secrets Configuration
# This file contains sensitive API keys and tokens
# Add this file to your .gitignore to keep secrets secure

# OpenAI API Configuration
OPENAI_API_KEY: "xxxxxxx"

# Anthropic API Configuration  
ANTHROPIC_API_KEY: "xxxxxxx"

# YouTube Transcribe Server Token
YOUTUBE_TRANSCRIBE_TOKEN: "xxxxxxx"

# Exa Search Server Token
EXA_SEARCH_TOKEN: "xxxxxxx"

# Brave Search API Key (optional - for brave_search server)
BRAVE_API_KEY: "xxxxxxx"


# Additional provider configurations can be added here
# Examples:

# Azure OpenAI Configuration
# AZURE_OPENAI_API_KEY: "your_azure_openai_key"
# AZURE_OPENAI_ENDPOINT: "https://your-resource.openai.azure.com"

# Google AI Configuration  
# GOOGLE_API_KEY: "your_google_api_key"

# DeepSeek Configuration
# DEEPSEEK_API_KEY: "your_deepseek_api_key"

# Groq Configuration
# GROQ_API_KEY: "your_groq_api_key"

# XAI Grok Configuration
# XAI_API_KEY: "your_xai_api_key"

# OpenRouter Configuration
# OPENROUTER_API_KEY: "your_openrouter_api_key"

# AWS Bedrock Configuration (uses AWS credentials)
# AWS_REGION: "us-east-1"
# AWS_PROFILE: "default"
